{
  "name": "mq3t",
  "version": "1.0.0-dev",
  "description": "MQTT Tools - A simple MQTT client for your development needs",
  "license": "GPL-3.0-only",
  "main": "./out/main/index.js",
  "author": "Guillaume Chx",
  "homepage": "https://mq3t.guillaumechx.dev/",
  "repository": "https://github.com/ChxGuillaume/MQ3T",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "npm run dependencies:license:parse && electron-vite preview",
    "dev": "npm run dependencies:license:parse && electron-vite dev",
    "build": "npm run dependencies:license:parse && npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "package:win": "electron-builder --win --config --publish never",
    "package:win:store": "node ./scripts/prepack.win.js && electron-builder --win --config electron-builder.appx.dist.yml --publish never",
    "package:mac": "electron-builder --mac --config --publish never",
    "package:mac:store": "node ./scripts/prepack.mas.js && electron-builder --mac --config electron-builder.mas.dist.yml --publish never",
    "package:linux": "electron-builder --linux --config --publish never",
    "package:linux:snap": "electron-builder --linux --config electron-builder.snap.yml --publish never",
    "package:all": "electron-builder -wlm --config --publish never",
    "package:win:publish": "electron-builder --win --publish always --config",
    "package:mac:publish": "electron-builder --mac --publish always --config",
    "package:linux:publish": "electron-builder --linux --publish always --config",
    "package:all:publish": "electron-builder -wlm --publish always --config",
    "package:all:publish:concurrent": "concurrently \"npm run package:win:publish\" \"npm run package:mac:publish\" \"npm run package:linux:publish\"",
    "clear:linux:electron:cache:mac": "rm ~/Library/Caches/electron/*linux*",
    "dependencies:license": "npx -y license-checker --production --json --out src/renderer/src/assets/licenses.json",
    "dependencies:license:parse": "npm run dependencies:license && node scripts/stripe-licenses.js src/renderer/src/assets/licenses.json"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@quasar/extras": "^1.17.0",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/core": "^1.45.1",
    "@vueuse/core": "^13.6.0",
    "axios": "^1.11.0",
    "echarts": "^5.6.0",
    "electron-updater": "^6.6.2",
    "exceljs": "^4.4.0",
    "highlight.js": "^11.11.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "monaco-editor": "^0.52.2",
    "mqtt": "^5.14.0",
    "pinia": "^3.0.3",
    "quasar": "^2.18.2",
    "uuid": "^11.1.0",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0",
    "xml-formatter": "^3.6.6",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@babel/traverse": ">=7.28.0",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@quasar/vite-plugin": "^1.10.0",
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/electron-devtools-installer": "^4.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.17.1",
    "@types/uuid": "^10.0.0",
    "@types/xml-formatter": "^2.1.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.21",
    "electron": "^36.7.4",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^3.1.0",
    "esbuild": "^0.25.9",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "less": "4.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "^1.90.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.2",
    "vite": "^5.4.19",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vue": "^3.5.18",
    "vue-tsc": "^2.2.12"
  }
}
